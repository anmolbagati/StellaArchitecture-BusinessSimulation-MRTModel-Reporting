			</aux>
			<aux name="Profitable Load">
				<eqn>No_of_Members_to_cover_Variable_Cost+No_of_Members_to_cover_Fixed_Cost</eqn>
				<units>People</units>
			</aux>
			<aux name="Potential Word of Mouth\nMembers">
				<eqn>&quot;Total_no._of_Members&quot;*&quot;Word_of_Mouth_(Factor)&quot;</eqn>
			</aux>
			<aux name="Total no. of Members">
				<eqn>Priority_Plus_Member+Priority_Member+Basic_Member</eqn>
			</aux>
			<aux name="Word of Mouth (Factor)">
				<eqn>1/500</eqn>
			</aux>
